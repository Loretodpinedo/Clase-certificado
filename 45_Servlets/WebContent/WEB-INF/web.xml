    <servlet-class>servlets.SegundoServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>SegundoServlet</servlet-name>
    <url-pattern>/SegundoServlet</url-pattern><!-- Esta es la url para ir a la web -->
